

<table>
  <tr>
    <th>Slug</th>
    <th>Target URL</th>
    <th>Created At</th>
  </tr>
  <% @links.each do |link| %>
    <tr>
      <td> <a href="<%= "/#{link.slug}" %>"> localhost:300/<%= link.slug %> </a></td>
      <td><%= link.target_url %></td>
      <td><%= link.created_at %></td>
    </tr>
  <% end %>
